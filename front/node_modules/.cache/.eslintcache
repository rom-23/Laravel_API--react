>\n        );\n    }\n}\n\nexport default (props) => (\n    <Picture\n        {...props}\n        params={useParams()}/>\n)\n","/home/rom/DEV/react-app/app_react/src/components/AppLoader.js",[],[],"/home/rom/DEV/api_laravel/front/src/index.js",[],[],"/home/rom/DEV/api_laravel/front/src/App.js",[],[],"/home/rom/DEV/api_laravel/front/src/reportWebVitals.js",[],[],"/home/rom/DEV/api_laravel/front/src/components/AppRouter.js",[],[],"/home/rom/DEV/api_laravel/front/src/components/Picture.js",["120","121"],[],"/home/rom/DEV/api_laravel/front/src/components/Register.js",[],[],"/home/rom/DEV/api_laravel/front/src/components/PostPicture.js",[],[],"/home/rom/DEV/api_laravel/front/src/components/Home.js",["122"],[],"/home/rom/DEV/api_laravel/front/src/components/Login.js",[],[],"/home/rom/DEV/api_laravel/front/src/components/Navbar.js",[],[],"/home/rom/DEV/api_laravel/front/src/components/AppLoader.js",[],[],{"ruleId":"123","replacedBy":"124"},{"ruleId":"125","replacedBy":"126"},{"ruleId":"127","severity":1,"message":"128","line":4,"column":15,"nodeType":"129","messageId":"130","endLine":4,"endColumn":24},{"ruleId":"131","severity":1,"message":"132","line":53,"column":37,"nodeType":"133","endLine":54,"endColumn":118},{"ruleId":"134","severity":1,"message":"135","line":76,"column":1,"nodeType":"136","endLine":80,"endColumn":2},{"ruleId":"131","severity":1,"message":"132","line":53,"column":37,"nodeType":"133","endLine":54,"endColumn":118},{"ruleId":"134","severity":1,"message":"135","line":76,"column":1,"nodeType":"136","endLine":80,"endColumn":2},{"ruleId":"127","severity":1,"message":"128","line":4,"column":15,"nodeType":"129","messageId":"130","endLine":4,"endColumn":24},"no-native-reassign",["137"],"no-negated-in-lhs",["138"],"no-unused-vars","'useParams' is defined but never used.","Identifier","unusedVar","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","import/no-anonymous